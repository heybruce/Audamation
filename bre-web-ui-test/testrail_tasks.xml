<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="TestRailJob" verbose="1" parallel="tests" thread-count="1">
    <test name="closeAllTestRunsForASpecificPeriod" parallel="classes" thread-count="1">
        <classes>
            <class name="utils.testrail.TestRailJob">
                <methods>
                    <include name="closeAllTestRunsForASpecificPeriod"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="deleteTestRunWithSuccessRateBelowPct" parallel="classes" thread-count="1">
        <classes>
            <class name="utils.testrail.TestRailJob">
                <methods>
                    <include name="deleteTestRunWithSuccessRateBelowPct"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>